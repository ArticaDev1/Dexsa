extends layout/layout

block variables
  -var title = sitename+' - Щитовое оборудование'

block content
  section.section.product-head.product-head_no-margin.screen.stp
    .container.lights-content 
      .bg-light.bg-light_size-1.bg-light_color-1
    .container.product-head__container
      .row.product-head__row
        h1.product-head__title.product-head__item.offset-lg-6.col-lg-6 Щитовое оборудование
        .product-head__image.offset-3.offset-lg-0.col-9.col-lg-5
          .product-head__image-wrapper.product-head__image-wrapper_type-6
            .image.image_contain
              img(data-src="./img/shhitovoe_oborudovanie/head.png", alt="image").lazyload

        .product-head__text.offset-lg-6.col-lg-6
          .row
            p.product-head__text.product-head__item.order-sm-2.order-lg-1.col-sm.col-md-8.col-lg-10 Первый в России взрывозащищённый светодиодный светильник с возобновляемум ресурсом работы!
            .order-sm-1.order-lg-2.col-sm-auto.col-md-4.col-lg-12
              a(href='#modal-form' data-modal='open').button.button_style-2.product-head__item 
                span Заказать
  
  section.section.product-separator 
    .container
      h2.product-separator__title Вводно-распределительные устройства

  section.section.product-item 
    .container.product-item__container
      //head
      .product-item__head.row 
        .product-item__head-title.col-12 
          h3.product-item__title-element Вводная панель 109
      //images
      -var images = ['./img/shhitovoe_oborudovanie/1_1.jpg']
      .product-item__images 
        .row
          .swiper.product-item__top-slider.offset-lg-0.offset-2.col-8.col-lg-12
            .swiper-container
              .swiper-wrapper
                each val in images
                  //slide
                  a(href=val data-fslightbox='1' data-barba-prevent data-cursor='magnifier').swiper-slide.image.image_contain.product-item__top-slide 
                    img(data-src=val, alt="image").swiper-lazy
            .swiper-pagination
      //content
      .product-item__content
        //схема
        .table.product-item__table 
          table 
            tr 
              th Модификация вводной панели:
              th Характеристики:
            tr 
              td(style='text-align:center;width:35%;') 100
              td Панель по схеме 1 без приборов учета Ш = 400 мм
            tr 
              td(style='text-align:center;width:35%;') 101
              td Панель по схеме 1 без приборов учета, Ш = 600 мм
            tr 
              td(style='text-align:center;width:35%;') 102
              td Панель по схеме 1 с приборами учета, Ш = 400 мм
            tr 
              td(style='text-align:center;width:35%;') 103
              td Панель по схеме 1 без приборов учета Ш = 400 мм


        .row.product-item__buttons
          //если доп информация существует то эта кнопка существует
          .col-md-6.col-lg-12
            button.product-item__more-button 
              span Показать подробные характеристики ↓
              span Свернуть подробные характеристики ↑
          //если пдф информация существует то эта кнопка существует
          .col-md-6.col-lg-12
            a(href='./docs/Prom-inn_A4_v3.pdf#page=11' target='_blank' rel='noopener').button.button_style-2 
              span Техническая документация (PDF) 
              
      //else content  
      .product-item__more-content
        //схема
        .table
          table 
            tr 
              th(style='text-align:left;' rowspan="2") Наименование параметра
              th(colspan="3" style='background-color:transparent;') Вид ВРУ
            tr
              th Многопанельное
              th Однопанельное
              th Шкафное
            tr
            tr 
              td Номинальное напряжение на вводе ВРУ, В
